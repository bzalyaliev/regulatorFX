<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.bzalyaliev.regulatorfx.controller.MainController">
    <children>
        <ListView fx:id="dataListView" layoutX="28.0" layoutY="44.0" prefHeight="200.0" prefWidth="544.0" />
        <Button fx:id="getDataButton" layoutX="28.0" layoutY="276.0" mnemonicParsing="false" onAction="#getData" text="Получить данные" />
        <TextField fx:id="temperatureField" layoutX="28.0" layoutY="320.0" promptText="Введите температуру" />
        <Button fx:id="setTemperatureButton" layoutX="190.0" layoutY="320.0" mnemonicParsing="false" onAction="#setTemperature" text="Задать" />
    </children>
</AnchorPane>